//---------------------------------------------------------------------------

#include <vcl.h>
#pragma hdrstop

#include "Unit30.h"
//---------------------------------------------------------------------------
#pragma package(smart_init)
#pragma resource "*.dfm"
Tiletisim *iletisim;
//---------------------------------------------------------------------------
__fastcall Tiletisim::Tiletisim(TComponent* Owner)
        : TForm(Owner)
{
}
//---------------------------------------------------------------------------

void __fastcall Tiletisim::Image2Click(TObject *Sender)
{
  String St = "http://www.gunalp.net/";
  ShellExecute(Handle,"open",St.c_str(),0,0,SW_SHOW);
}
//---------------------------------------------------------------------------

void __fastcall Tiletisim::Image3Click(TObject *Sender)
{
  String St = "mailto:tarik@gunalp.net?subject=KONU";
  ShellExecute(Handle,"open",St.c_str(),0,0,SW_SHOW);
}
//---------------------------------------------------------------------------

