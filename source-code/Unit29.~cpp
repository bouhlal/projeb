void __fastcall TBetonoku::ToolButton5Click(TObject *Sender)
{
int Boyut;
int Handle;
int Yer;
Yer = ftell(Dosyabo);
Handle = open("\\Proje B\\Data\\Betonarme.prjb", O_APPEND);
Boyut = filelength(Handle);
if (Boyut == Yer )
   ShowMessage("Dosyanýn sonundasýnýz");
   else
   {
   fseek(Dosyabo, 0, SEEK_CUR);
   fread(&Adres_kaydibo, sizeof(Yapibo), 1, Dosyabo);
StringGrid1->Cells[1][3] = String(Adres_kaydibo.Don1);
