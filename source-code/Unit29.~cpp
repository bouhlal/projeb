     StringGrid1->Cells[8][2] = "Zi (cm)";
      StringGrid1->Cells[9][2] = "Fsi.Zi";
       Edit11->Text = "No:";
    if ((Dosyabo = fopen("\\Proje B\\Data\\Betonarme.prjb", "rb")) == NULL)
     ShowMessage("Proje B\\Data\\Betonarme.prjb dosyasý bulunamadý.");
