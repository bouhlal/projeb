//---------------------------------------------------------------------------

void __fastcall TBetonoku::ToolButton1Click(TObject *Sender)
{
  if (PrintDialog1->Execute()){
    try {
        Betonoku->Print();
    }
    catch(...){
        Printer()->EndDoc();
        throw;
    }
  }
}
